{
  "name": "koded/http",
  "type": "library",
  "license": "BSD-3-Clause",
  "description": "Koded HTTP component.",
  "keywords": [
    "request",
    "response",
    "http",
    "http-client",
    "stream",
    "negotiation",
    "psr-7",
    "psr-17"
  ],
  "authors": [
    {
      "name": "Mihail Binev",
      "homepage": "http://kodeart.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "psr/http-message": "~1",
    "psr/http-factory": "~1",
    "koded/stdlib": "~4",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "autoload": {
    "psr-4": {
      "Koded\\Http\\": ""
    },
    "files": [
      "functions.php",
      "HttpFactory.php"
    ],
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "suggest": {
    "ext-iconv": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7",
    "php-http/psr7-integration-tests": "^1.0@dev",
    "codacy/coverage": "dev-master"
  },
  "autoload-dev": {
    "psr-4": {
      "Koded\\Http\\": "Tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    }
  }
}