{
  "name": "koded/http",
  "type": "library",
  "license": "BSD-3-Clause",
  "description": "Koded HTTP component.",
  "keywords": ["request", "response", "http", "stream", "php7"],
  "authors": [
    {
      "name": "Mihail Binev",
      "homepage": "http://kodeart.com"
    }
  ],
  "require": {
    "php": "~7.1",
    "koded/stdlib": "~1",
    "psr/http-message": "~1"
  },
  "autoload": {
    "psr-4": {
      "Koded\\Http\\": ""
    },
    "files": [
      "functions.php"
    ],
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "~6",
    "satooshi/php-coveralls": "~1"
  },
  "autoload-dev": {
    "psr-4": {
      "Koded\\Http\\": "Tests/"
    },
    "files": [
      "vendor/koded/stdlib//functions-dev.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.7.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
